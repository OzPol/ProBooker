{
  "presets": [
    "next/babel",
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@app": "./app",
          "@components": "./app/components",
          "@lib": "./app/lib",
          "@pages": "./pages",
          "@services": "./services",
          "@tests": "./tests"
        }
      }
    ]
  ]
}
